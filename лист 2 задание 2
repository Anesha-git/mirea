#include <iostream>
using namespace std;

int main() {
    int x1, y1, x2, y2;
    
    cout << "Введите координаты ладьи";
    cin >> x1 >> y1;
    
    cout << "Введите координаты фигуры";
    cin >> x2 >> y2;
    
    if (x1 == x2 || y1 == y2) {
        cout << "Ладья бьет" << endl;
    } else {
        cout << "Ладья НЕ бьет" << endl;
    }
    
    return 0;
}
