#include <iostream>
using namespace std;

int main() {
    int x;
    cin >> x;
    
    // Перебираем числа от 2 до x
    for (int i = 2; i <= x; i++) {
        // Если i делит x без остатка
        if (x % i == 0) {
            cout << i << endl;
            break;
        }
    }
    
    return 0;
}
